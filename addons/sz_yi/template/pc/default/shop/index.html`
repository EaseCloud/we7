<script>
        //插件点击隐藏或者展示按钮
        $(".j-cover-page-plug-in div").bind("mouseenter mouseleave",function(e){
            if(e.type == "mouseenter"){
                $(this).find(".btn").show();
            }else{
                $(this).find(".btn").hide();
